unsig=0A         break;=0A =0A     case 0x80000008:=0A+        *eax &=3D =
0xff;=0A         count =3D d->arch.paging.gfn_bits + PAGE_SHIFT;=0A-       =
 if ( (*eax & 0xff) > count )=0A-            *eax =3D (*eax & ~0xff) | =
count;=0A+        if ( *eax > count )=0A+            *eax =3D count;=0A =
=0A         hvm_cpuid(1, NULL, NULL, NULL, &_edx);=0A         count =3D =
