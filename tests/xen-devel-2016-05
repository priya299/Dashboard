x86: cap address bits CPUID output=0A=0ADon't use more or report more to =
guests than we are capable of=0Ahandling.=0A=0AAt once simplify the code =
in hvm_cpuid() and mtrr_top_of_ram().=0A=0ASigned-off-by: Jan Beulich =
<jbeulich@suse.com>=0A=0A--- a/xen/arch/x86/cpu/common.c=0A+++ b/xen/arch/x=
86/cpu/common.c=0A@@ -46,6 +46,7 @@ const struct cpu_dev *__read_mostly =
cpu_=0A =0A unsigned int paddr_bits __read_mostly =3D 36;=0A unsigned int =
hap_paddr_bits __read_mostly =3D 36;=0A+unsigned int vaddr_bits __read_most=
ly =3D VADDR_BITS;=0A =0A /*=0A  * Default host IA32_CR_PAT value to cover =
all memory types.=0A@@ -240,7 +241,14 @@ static void __init early_cpu_detec=
t(void=0A 	if ( cpuid_eax(0x80000000) >=3D 0x80000008 ) {=0A 		=
eax =3D cpuid_eax(0x80000008);=0A 		paddr_bits =3D eax & =
0xff;=0A+		if (paddr_bits > PADDR_BITS)=0A+			=
paddr_bits =3D PADDR_BITS;=0A+		vaddr_bits =3D (eax >> 8) & =
0xff;=0A+		if (vaddr_bits > VADDR_BITS)=0A+			=
vaddr_bits =3D VADDR_BITS;=0A 		hap_paddr_bits =3D ((eax >> 16) & =
0xff) ?: paddr_bits;=0A+		if (hap_paddr_bits > PADDR_BITS)=0A=
+			hap_paddr_bits =3D PADDR_BITS;=0A 	}=0A }=0A =
=0A--- a/xen/arch/x86/e820.c=0A+++ b/xen/arch/x86/e820.c=0A@@ -451,11 =
+451,11 @@ static uint64_t __init mtrr_top_of_ram(v=0A          return =
0;=0A =0A     /* Find the physical address size for this CPU. */=0A-    =
cpuid(0x80000000, &eax, &ebx, &ecx, &edx);=0A-    if ( eax >=3D 0x80000008 =
)=0A+    if ( cpuid_eax(0x80000000) >=3D 0x80000008 )=0A     {=0A-        =
cpuid(0x80000008, &eax, &ebx, &ecx, &edx);=0A-        phys_bits =3D =
(uint8_t)eax;=0A+        phys_bits =3D (uint8_t)cpuid_eax(0x80000008);=0A+ =
       if ( phys_bits > PADDR_BITS )=0A+            phys_bits =3D =
PADDR_BITS;=0A     }=0A     addr_mask =3D ((1ull << phys_bits) - 1) & =
~((1ull << 12) - 1);=0A =0A--- a/xen/arch/x86/hvm/hvm.c=0A+++ b/xen/arch/x8=
